analog function real limitex;
    input x;
    real x;
    begin
        if (x > 17.) begin
            limitex = 42. * (1.0+(x)-17.);
        end else if (x < -17.) begin
            limitex = 42.;
        end else begin
            limitex = exp(x);
        end
    end
endfunction

analog function real fct;
    real x;
    input x;
    begin
        fct = ln(max(x , .1));
    end
endfunction

