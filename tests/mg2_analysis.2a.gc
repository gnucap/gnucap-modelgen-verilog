module test_an2a(np, nn);
	electrical np, nn;
	inout np, nn;
	analog begin
		if(analysis("ic"))
			V(np,nn) <+ 1;
		else
			I(np,nn) <+ sin(ddt(V(np,nn))/100.)*100.;
	end
endmodule

