
attach ./modelgen_0.so
load ../vams/vsine.so

verilog

`modelgen
module test_xdt0p(out, in);
  electrical out, in;
parameter real c=1;
  analog begin
    I(out) <+ ddt(c*V(in));
  end
endmodule

!make test_xdt0p.so > /dev/null

attach ./test_xdt0p.so

verilog

test_xdt0p #(.c(c)) dut(out, in);
resistor #(.r(1)) r(0, out);

parameter v=0
parameter c=1m
vsine #(.dc(v), .ampl(1)) v1(in, 0);

print tran v(in) v(r) iter(0)
tran .5
status notime
