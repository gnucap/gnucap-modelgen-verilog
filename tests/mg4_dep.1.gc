
attach ./modelgen_0.so


verilog

`modelgen
module test_dep1(p, n);
	electrical p, n;
	inout p, n;
	real i;
	(*desc="prb"*) real prb;
	localparam real p1 = 1;
	analog begin : main
		real j;
		i = 0;

		if(p1) begin
			i = V(p,n);
		end

		j = i;
		prb = ddx(j, V(p,n));
		I(p,n) <+ j;
	end
endmodule

!make test_dep1.so > /dev/null
attach ./test_dep1.so

parameter v=1

test_dep1 #() dut(1,0);
vsource #(.dc(v)) v1(1, 0);

list

print dc i(v1) prb(dut)

dc
end
