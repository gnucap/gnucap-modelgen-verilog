nature current
  access = "I";
  units = "A";
  abstol = "0.001234";
endnature

nature voltage
  access = "V";
  units = "V";
  abstol = "0.0004321";
endnature


discipline electrical
  potential "voltage";
  flow "current";
enddiscipline


module test_portbranch(p, n);
  electrical p, n;
  real i;

  analog begin
    V(n, p) <+ 0.;
    i = flow(<p>);
  end

endmodule



