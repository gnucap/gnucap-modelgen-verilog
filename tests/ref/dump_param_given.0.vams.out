nature current
  access = "I";
  units = "A";
  abstol = "0.001234";
endnature

nature voltage
  access = "V";
  units = "V";
  abstol = "0.0004321";
endnature


discipline electrical
  potential "voltage";
  flow "current";
enddiscipline


module test_pg0(p, n);
  electrical p, n;
  parameter real st = 3;

  analog begin
    if ($param_given(st)) begin
      V(p, n) <+ 1;
    end
    else
    V(p, n) <+ 2;
  end

endmodule



