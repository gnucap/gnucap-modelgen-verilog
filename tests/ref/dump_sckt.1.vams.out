nature current
  access = "I";
  units = "A";
  abstol = "0.001234";
endnature

nature voltage
  access = "V";
  units = "V";
  abstol = "0.0004321";
endnature


discipline electrical
  potential "voltage";
  flow "current";
enddiscipline


module test_vs0(a, b, gnd);
  inout a, b, gnd;
  electrical a, b, gnd;
  vsource #(.dc(1)) v1(.p(a), .n(gnd));
  vsource #(.dc(1)) v2(.n(gnd), .p(a));

endmodule



