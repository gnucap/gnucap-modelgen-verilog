nature current
  access = "I";
  units = "A";
  abstol = "0.001234";
endnature

nature voltage
  access = "V";
  units = "V";
  abstol = "0.0004321";
endnature


discipline electrical
  potential "voltage";
  flow "current";
enddiscipline


module ternary(p, n);
  inout p, n;
  electrical p, n;
  parameter real c = 1.;

  real   a;

  analog begin
    real b;
    real N, i, WE, WSEG;
    a = (((((N * i) > 1.))? (1):(2)) + 1);
  end

endmodule



