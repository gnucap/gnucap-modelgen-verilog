

module test_af();
  analog function real limitex;
    input x;
    real x;
    begin
      if ((x > 17.)) begin
        limitex = ((x + -16.) * 42.);
      end
      else begin
        if ((x < -17.)) begin
          limitex = 42.;
        end
        else begin
          limitex = exp(x);
        end
      end
    end
  endfunction

  analog function real strange_log;
    input x;
    real x;
    begin
      strange_log = ln(max(x, .1));
    end
  endfunction

  analog function real something;
    input x, y;
    real x, y;
    begin
      something = ((x + sqrt(((x * x) + ((y * 4.0) * y)))) * 0.5);
    end
  endfunction

  analog begin
    real i;
    i = something( 26.,  20.0855369231877);
    i = something( 26.,  1.);
  end

endmodule



