      end
      else if ((x < -17.)) begin
        limitex = 42.;
      end
      else begin
        limitex = exp(x);
      end
    end
  endfunction

  analog function real strange_log;
    input x;
    real x;
begin
      strange_log = ln(max(x, .1));
    end
  endfunction

  analog function real something;
    input x, y;
    real x, y;
begin
