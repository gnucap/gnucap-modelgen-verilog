nature current
  access = "I";
  units = "A";
  abstol = "0.001234";
endnature

nature voltage
  access = "V";
  units = "V";
  abstol = "0.0004321";
endnature


discipline electrical
  potential "voltage";
  flow "current";
enddiscipline


module test(p, n);
  electrical p, n;
  branch (p, n) br_pn;
  (* desc=v *)
  real   v;

  analog begin
    v = V(p, n);
    I(br_pn) <+ V(br_pn);
  end

endmodule



