nature current
  access = "I";
  units = "A";
  abstol = "0.001234";
endnature

nature voltage
  access = "V";
  units = "V";
  abstol = "0.0004321";
endnature


discipline electrical
  potential "voltage";
  flow "current";
enddiscipline


module device(p, n);
  inout p, n;
  real   r;

  analog begin
    @(initial_step)
      r = 3;
  end

  analog begin
    @(initial_step)     begin
      r = 2;
    end
  end

endmodule



