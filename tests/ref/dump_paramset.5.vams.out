module resistor(a, b);
  electrical a, b;
  parameter real r = 1.;

  aliasparam R = r;

  analog begin
    I(a, b) <+ (V(a, b) / r);
  end

endmodule



// paramsets
module my_res(a, b);
  electrical a, b;
  parameter real r = 1.;
  localparam real __r = r;

  analog begin
    I(a, b) <+ (V(a, b) / __r);
  end

endmodule

