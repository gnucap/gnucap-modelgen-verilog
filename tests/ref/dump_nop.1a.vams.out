nature current
  access = "I";
  units = "A";
  abstol = "0.001234";
endnature

nature voltage
  access = "V";
  units = "V";
  abstol = "0.0004321";
endnature


discipline electrical
  potential "voltage";
  flow "current";
enddiscipline


module test(a, b);
  electrical a, b;
  parameter real arg = 0 from [(0.):(0.)];

  real   T;
  real   U;

  analog begin
    begin
      V(a) <+ 2.;
    end
    real x;
    x = 0.;
    if (x) begin
      V(a) <+ 5.;
    end
    case (arg)
      0: begin
        V(b) <+ 1.2;
      end
    endcase
    case (arg)
      0: begin
        V(b) <+ 2.0;
      end
    endcase
  end

endmodule



