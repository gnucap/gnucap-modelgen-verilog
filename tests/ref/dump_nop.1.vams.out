nature current
  access = "I";
  units = "A";
  abstol = "0.001234";
endnature

nature voltage
  access = "V";
  units = "V";
  abstol = "0.0004321";
endnature


discipline electrical
  potential "voltage";
  flow "current";
enddiscipline


module test(a, b);
  electrical a, b;
  parameter real arg = 0 from [(0.):(0.)];

  real   T;
  real   U;

  analog begin
    if (0) begin
      V(a) <+ 1.;
    end
    else begin
      V(a) <+ 2.;
    end
    if (arg) begin
      V(b) <+ 3.;
    end
    if ( 0.) begin
      V(a) <+ 4.;
    end
    case (arg)
      0: begin
        V(b) <+ 2.2;
      end
      default: ;
    endcase
  end

endmodule



