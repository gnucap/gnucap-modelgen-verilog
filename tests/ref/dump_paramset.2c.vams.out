module resistor(a, b);
  electrical a, b;
  parameter real r =  1.;

  analog begin
    I(a, b) <+ (V(a, b) / r);
  end

endmodule



// paramsets
module my_res(a, b);
  electrical a, b;
  parameter real R =  1. from [( 1.):( 1.)];
  localparam real __r = R;

  analog begin
    I(a, b) <+ V(a, b);
  end

endmodule

