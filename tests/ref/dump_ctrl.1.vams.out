nature current
  access = "I";
  units = "A";
  abstol = "0.001234";
endnature

nature voltage
  access = "V";
  units = "V";
  abstol = "0.0004321";
endnature


discipline electrical
  potential "voltage";
  flow "current";
enddiscipline


module while0(p, n);
  analog begin
    real S, T, U;
    while (0);
    while (S);
    while (S) begin
      S = 0;
    end
  end

endmodule
module while1(p, n);
  analog begin
    real S;
    while ((S < 10)) begin
      S = (S + 1);
    end
  end

endmodule



