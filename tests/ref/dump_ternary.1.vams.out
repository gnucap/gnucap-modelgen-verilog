nature current
  access = "I";
  units = "A";
  abstol = "0.001234";
endnature

nature voltage
  access = "V";
  units = "V";
  abstol = "0.0004321";
endnature


discipline electrical
  potential "voltage";
  flow "current";
enddiscipline


module ternary(p, n);
  inout p, n;
  electrical p, n;
  parameter real c = 1.;

  real a;

  analog begin
    a = V(n, p);
    I(p, n) <+ ((c)? (a):(a));
  end

endmodule



