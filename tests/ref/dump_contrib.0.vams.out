nature current
  access = "I";
  units = "A";
  abstol = "0.001234";
endnature

nature voltage
  access = "V";
  units = "V";
  abstol = "0.0004321";
endnature


discipline electrical
  potential "voltage";
  flow "current";
enddiscipline


module contrib(p);
  inout p;
  electrical p;
  analog begin
    I(p) <+ V(p);
  end

endmodule



