nature current
  access = "I";
  units = "A";
  abstol = "0.001234";
endnature

nature voltage
  access = "V";
  units = "V";
  abstol = "0.0004321";
endnature


discipline electrical
  potential "voltage";
  flow "current";
enddiscipline


module device(p, n);
  inout p, n;
  parameter real c = 1.;
  localparam real l1 = (c + 5), l2 = 10;
  parameter real d = 1., e = 2;
  parameter integer i = 0, j = 10;
  localparam real t1 = exp(l1), t2 = 10;

  analog ;

endmodule
module device(p, n);
  inout p, n;
  parameter real c = 1. from [(0.):(inf));
  parameter real d = 1. from [(1.):(2.)] exclude 3.;
  parameter real e = 1. from [(0.):(inf)), f = 3;

  aliasparam g = c;
  aliasparam h = e;

endmodule



