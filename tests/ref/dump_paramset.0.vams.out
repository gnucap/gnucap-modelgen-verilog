nature current
  access = "I";
  units = "A";
  abstol = "0.001234";
endnature

nature voltage
  access = "V";
  units = "V";
  abstol = "0.0004321";
endnature


discipline electrical
  potential "voltage";
  flow "current";
enddiscipline


(* attribute="proto" *)
module resistor(a, b);
  electrical a, b;
  parameter real r = 1;

  analog begin
    I(a, b) <+ (V(a, b) / r);
  end

endmodule



(* attribute="stub" *)
paramset const_res resistor;
endparamset;

