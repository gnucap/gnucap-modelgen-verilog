nature current
  access = "I";
  units = "A";
  abstol = "0.001234";
endnature

nature voltage
  access = "V";
  units = "V";
  abstol = "0.0004321";
endnature


discipline electrical
  potential "voltage";
  flow "current";
enddiscipline


(* attribute="proto" *)
module resistor(a, b);
  electrical a, b;
  (* attrib=yes *)
  parameter real r = 0.;
  parameter real a = 0, b = 0.;

  analog begin
    I(a, b) <+ (V(a, b) / r);
  end

endmodule



// paramsets
(* attribute="stub" *)
paramset const_res resistor;
  parameter real rr = 0., RR = 0.;
  localparam real ll = rr;
  (* attrib=yes *)
  parameter real xx = 0.;

  .r = 1
  .a = (xx + ll)
endparamset;

