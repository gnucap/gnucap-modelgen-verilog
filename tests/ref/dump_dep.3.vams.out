module test_while0a(p, n);
  inout p, n;
  electrical p, n;
  parameter real r = 0;

  real   z
;

  analog begin // always
    // i : 0
    // x : 1
    // y : 1
    // z : 0
    integer i;
    real x, y;
    x = 0.;
    x = 0.; // dep: _potential_b_p_n
    z = x;
    y = 0.; // dep: _potential_b_p_n
    i = r;
    while ((i > 0)) begin
      // i : 0
      // x : 1
      // y : 1
      i = (i - 1);
      x = y; // dep: _potential_b_p_n
      y = (x + V(p, n)); // dep: _potential_b_p_n
    end
    x = x; // dep: _potential_b_p_n
    I(p, n) <+ x; // dep: _potential_b_p_n
  end

endmodule



