module foo(p, x);
  inout p;
  electrical p;
  electrical x;
  real A, B;
  real y// dep _potential_b_p_
;

  analog begin : main // always
    // A : 0
    // B : 0
    // C : 0
    // y : 0
    real C;
    A = 1; // s1 r2
    B = A; // s2 r2
    C = B; // s3 r2
    $strobe("some strobe %f.", (C + V(x))); // dep: _potential_b_x_ s3 r2
    y = V(p); // dep: _potential_b_p_ s1 r2
