module dep7(d, g);
  inout d, g;
  electrical d, g;
  parameter real p = 0.;

  real T;

  analog begin : main // always
    // T : 0
    // c : 0
    real c;
    @(initial_step) begin
      if ((! $param_given(p))) begin
        // c : 0
        c = 1.; // s2 r1
      end
    end
    T = c; // s4
    I(d, g) <+ c; // s3 r1
  end

endmodule



