nature current
  access = "I";
  units = "A";
  abstol = "0.001234";
endnature

nature voltage
  access = "V";
  units = "V";
  abstol = "0.0004321";
endnature


discipline electrical
  potential "voltage";
  flow "current";
enddiscipline


(* test="string", test2=1 *)
module device(p, n);
  inout p, n;
  (* desc="normal port" *)
  electrical p, n;
  parameter real c = 1.;
  (* units="m^2", type="instance", desc="Bottom area of source junction" *)
  parameter real ABSOURCE = 1.0e-12;

  (* desc="dummy" *)
  real    x;

  analog begin
  end

endmodule



