nature current
  access = "I";
  units = "A";
  abstol = "0.001234";
endnature

nature voltage
  access = "V";
  units = "V";
  abstol = "0.0004321";
endnature

nature power
  access = "Pwr";
  units = "W";
  abstol = "0.00101010";
endnature

nature flux
  access = "Phi";
  units = "We";
endnature

nature temperature
  access = "Temp";
  units = "K";
  abstol = "3.14";
endnature


discipline thermal
  potential "temperature";
  flow "power";
enddiscipline

discipline electrical
  potential "voltage";
  flow "current";
enddiscipline

discipline magnetic
  potential "voltage";
  flow "current";
enddiscipline

discipline rotational_omega
  potential "voltage";
  flow "current";
enddiscipline


module heat(t);
  inout t;
  thermal t;
  real    tt;

  analog begin
    tt = Temp(t);
  end

endmodule



