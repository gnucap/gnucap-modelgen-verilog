`modelgen
module test_evt3a(p, n);
	electrical p, n;
	inout p, n;
	(*desc=""*) integer cnt;
	analog begin
		@(timer(0,2)) begin
			$debug("timerd %g\n", $abstime, cnt);
			$write("timer0 %g\n", $abstime, cnt);
			cnt = cnt + 1;
			$write("timer1 %g\n", $abstime, cnt);
      end
	end
endmodule
`endmodelgen

