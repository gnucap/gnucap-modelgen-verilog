// Test diode model

verilog

attach ../d_vasrc.so
attach ../vams/diode.so

module main(n, g)
	diode #(.cjo(1pf) .tf(1u)) D1(n g);
endmodule

main m(n,m);
resistor #(.r(1k)) r(m,0);

spice


.parameter v=0
V1  (n, 0) vsource dc=v ac=1
* D1  (n m) dd
* .model dd d cjo=1pf is=1e-15 tt=1u

.print dc v(nodes) i(V1)
.dc v -1.4 .7 .7

.print ac v(nodes)
.ac 1 1000k * 10

.status notime
